';\nimport {Home} from \"./pages/home\";\nimport {Switch, Route, useHistory, Redirect} from \"react-router-dom\";\nimport {MovieDetails} from \"./pages\";\n\n//show error handler for react components\n// notifications\n\nfunction App() {\n\n    const history = useHistory();\n  return (\n      <BaseLayout>\n          <Switch>\n              <Route path={'/'} exact>\n                  < Home/>\n              </Route>\n\n              <Route path={'/movie/:id'}>\n                  <MovieDetails/>\n              </Route>\n\n              {/*<Redirect to = '/' />*/}\n\n              <Route>\n                  <h1>PATH NOT FOUND\n                      <button onClick={() => history.push('/')}>go home</button>\n                  </h1>\n              </Route>\n          </Switch>\n\n\n      </BaseLayout>\n  );\n}\n\nexport default App;\n","/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/services/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/services/MoviesService.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/services/axiosConfig.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/layouts/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/layouts/BaseLayout.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/components/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/components/film-item/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/components/film-list/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/components/film-item/FilmItem.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/components/film-list/FilmList.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/services/GenresService.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/pages/home/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/pages/home/Home.js",["84","85","86","87"],"import React, {useEffect, useState} from \"react\";\nimport {FilmList} from \"../../components\";\nimport {moviesService, genresService} from '../../services';\nimport  styles from './Home.module.css';\nimport {useHistory} from \"react-router-dom\";\n\n\nexport const Home = () => {\n    const history = useHistory();\n    const [moviesList, setMoviesList] = useState([]);\n    const [isLoading, setIsLoading] = useState(null);\n\n    const fetchMovies = async () => {\n        try {\n            const {results, page, total_pages, total_results} = await moviesService.getMovies();\n\n            return results;\n        } catch (e) {\n            console.error(e);\n\n        }\n    }\n\n    const fetchGenres = async () => {\n\n       try {\n           const {genres} = await genresService.getGenres();\n\n           return genres;\n    } catch (e) {\n           console.error(e)\n       }\n    }\n\n    const fetchMoviesData = async () => {\n        const requests = [fetchMovies(), fetchGenres()];\n\n        try {\n            setIsLoading(true);\n            const [movies, genres] = await Promise.all(requests)\n            // console.log({movies, genres});\n\n            const mergedWithGenresMovies = movies.map((movie) => {\n                const {genre_ids} = movie;\n                const movieGenresList = genre_ids.map(genreId => genres.find(el => el.id === genreId ))\n                return {...movie, movieGenresList }\n            })\n\n            // console.log(mergedWithGenresMovies);\n            setMoviesList(mergedWithGenresMovies);\n\n        } catch (e) {\n            console.error(e);\n        } finally {\n            setIsLoading(false)\n        }\n    }\n\n    useEffect(() => {\n        fetchMoviesData()\n    },[])\n\n    const renderLoadingIndicator = () => (\n        <div className={styles.loading}> Loading...</div>\n    )\n\n    const onFilmClick = (film) => {\n        history.push(`/movie/${film.id}`)\n    }\n    return(\n        <div>\n            {/*{ true ? renderLoadingIndicator() :  <FilmList/> }*/}\n\n            { isLoading || isLoading === null ? renderLoadingIndicator() : (\n                <FilmList\n                    onFilmClick = {onFilmClick}\n                    items={moviesList}  />\n            )}\n        </div>\n    )\n}","/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/pages/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/pages/movie-details/index.js",[],"/Users/macbooc/WebstormProjects/newProj/react/tz-react/src/pages/movie-details/MovieDetails.js",["88","89"],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":5,"column":36,"nodeType":"96","messageId":"97","endLine":5,"endColumn":44},{"ruleId":"94","severity":1,"message":"98","line":15,"column":29,"nodeType":"96","messageId":"97","endLine":15,"endColumn":33},{"ruleId":"94","severity":1,"message":"99","line":15,"column":35,"nodeType":"96","messageId":"97","endLine":15,"endColumn":46},{"ruleId":"94","severity":1,"message":"100","line":15,"column":48,"nodeType":"96","messageId":"97","endLine":15,"endColumn":61},{"ruleId":"101","severity":1,"message":"102","line":61,"column":7,"nodeType":"103","endLine":61,"endColumn":9,"suggestions":"104"},{"ruleId":"94","severity":1,"message":"105","line":2,"column":20,"nodeType":"96","messageId":"97","endLine":2,"endColumn":33},{"ruleId":"101","severity":1,"message":"106","line":27,"column":7,"nodeType":"103","endLine":27,"endColumn":9,"suggestions":"107"},"no-native-reassign",["108"],"no-negated-in-lhs",["109"],"no-unused-vars","'Redirect' is defined but never used.","Identifier","unusedVar","'page' is assigned a value but never used.","'total_pages' is assigned a value but never used.","'total_results' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'fetchMoviesData'. Either include it or remove the dependency array.","ArrayExpre